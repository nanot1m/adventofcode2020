  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Go Program",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${fileDirname}",
      "envFile": "${workspaceFolder}/.env"
    },
    {
      "name": "Launch Node Program",
      "program": "${file}",
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "envFile": "${workspaceFolder}/.env",
      "type": "node"
    }
  ]
}
